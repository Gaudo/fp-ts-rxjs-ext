{
	"name": "@fgaudo/fp-ts-rxjs",
	"version": "0.0.1",
	"description": "Observables with fp-ts",
	"license": "MIT",
	"author": "Francesco Gaudenzi <fgaudo@gmail.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/fgaudo/fp-ts-rxjs.git"
	},
	"bugs": {
		"url": "https://github.com/fgaudo/fp-ts-rxjs/issues",
		"email": "fgaudo@gmail.com"
	},
	"exports": {
		".": {
			"import": {
				"types": "./dist/esm/types/index.d.ts",
				"default": "./dist/esm/index.mjs"
			},
			"require": {
				"types": "./dist/cjs/types/index.d.ts",
				"default": "./dist/cjs/index.js"
			}
		}
	},
	"main": "./dist/cjs/index.js",
	"files": [
		"dist/**/*"
	],
	"scripts": {
		"build": "npm run clean && npm run build:esm && npm run build:cjs",
		"build:cjs": "tsc -p ./tsconfig.cjs.json",
		"build:esm": "tsc -p ./tsconfig.esm.json && move dist/esm/index.js dist/esm/index.mjs",
		"clean": "del ./dist"
	},
	"types": "./dist/cjs/types/index.d.ts",
	"peerDependencies": {
		"fp-ts": "2.x",
		"rxjs": "7.x"
	},
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"@types/chai": "^4.3.11",
		"@types/mocha": "^10.0.6",
		"@typescript-eslint/eslint-plugin": "^6.18.1",
		"@typescript-eslint/parser": "^6.18.1",
		"chai": "^5.0.0",
		"del-cli": "^5.1.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-functional": "^6.0.0",
		"eslint-plugin-tsdoc": "^0.2.17",
		"mocha": "^10.2.0",
		"move-cli": "^2.0.0",
		"prettier": "^3.1.1",
		"simplytyped": "^3.3.0",
		"ts-mocha": "^10.0.0",
		"typescript": "^5.3.3"
	},
	"keywords": [
		"fp-ts",
		"fp-ts-rxjs",
		"rxjs"
	],
	"tags": [
		"fp-ts-rxjs",
		"rxjs",
		"fp-ts"
	]
}
